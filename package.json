{
  "name": "hunters-run",
  "version": "0.1.0",
  "private": true,
  "description": "Hunters Run monorepo (apps + packages)",
  "engines": {
    "node": "22.18.0",
    "npm": ">=10.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "npm -ws run postinstall --if-present && husky",
    "build": "npm -ws run build --if-present",
    "dev:hr": "npm --workspace @hunters-run/api run start:dev",
    "migrate": "npm --workspace @hunters-run/db run migrate",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "lint": "npm -ws run lint --if-present",
    "test": "npm -ws run test --if-present",
    "format": "prettier --write \"**/*.{js,ts,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md,yaml,yml}\"",
    "lint:fix": "npm -ws run lint:fix --if-present",
    "precommit": "lint-staged",
    "prepare": "husky",
    "mb:migrate": "node scripts/run-sql-migrations.js",
    "mb:migrate:status": "node scripts/run-sql-migrations.js status",
    "mb:outbox": "node apps/hr-api/src/workers/outbox.worker.js",
    "mb:outbox:daemon": "node apps/hr-api/src/workers/outbox.worker.js --daemon",
    "mb:checkins": "node apps/hr-api/src/workers/due-checkin.worker.js",
    "mb:digest": "node apps/hr-api/src/workers/weekly-digest.worker.js"
  },
